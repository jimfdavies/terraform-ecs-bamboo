[
  {
    "name": "bamboo-java-agent",
    "image": "atlassian/bamboo-java-agent:latest",
    "cpu": 10,
    "memory": 512,
    "portMappings": [
      {
        "hostPort": 8085,
        "containerPort": 8085,
        "protocol": "tcp"
      },
      {
        "hostPort": 54663,
        "containerPort": 54663,
        "protocol": "tcp"
      }
    ],
    "environment": [
        {
          "name": "BAMBOO_SERVER",
          "value": "http://10.75.0.52:8052/bamboo"
        },
        {
          "name": "HOME",
          "value": "/root/"
        }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group_name}",
            "awslogs-region": "${log_group_region}"
        }
    },
    "essential": true
  }
]
