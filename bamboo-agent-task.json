[
  {
    "name": "bamboo-java-agent",
    "image": "jimfdavies/bamboo-agent-alpine:latest",
    "cpu": 10,
    "memory": 1024,
    "environment": [
        {
          "name": "BAMBOO_SERVER",
          "value": "http://10.75.0.52:8085/bamboo"
        },
        {
          "name": "HOME",
          "value": "/root/"
        }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group_name}",
            "awslogs-region": "${log_group_region}"
        }
    }
  }
]
