[
  {
    "name": "bamboo-server",
    "image": "jimfdavies/bamboo-server-alpine:latest",
    "cpu": 10,
    "memory": 4096,
    "portMappings": [
      {
        "hostPort": 8085,
        "containerPort": 8085,
        "protocol": "tcp"
      },
      {
        "hostPort": 54663,
        "containerPort": 54663,
        "protocol": "tcp"
      }
    ],
    "environment": [
        {
          "name": "BAMBOO_VERSION",
          "value": "6.1.1"
        }
    ],
    "essential": true,
    "mountPoints": [
      {
        "containerPath": "/home/bamboo",
        "sourceVolume": "efs-bamboo-home"
      }
    ]
  }
]
